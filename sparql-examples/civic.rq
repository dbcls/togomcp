# Endpoint: http://sparql.med2rdf.org/sparql
# Description: Get gene variants and evidences

PREFIX m2r: <http://med2rdf.org/ontology/med2rdf#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX civico: <http://purl.jp/bio/10/civic/ontology#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT ?ncbigene ?variation ?variant_origin ?variation_label ?pubmed
FROM <http://med2rdf.org/graph/civic>
WHERE {
    ?Gene a m2r:Gene ;
        rdfs:seeAlso ?ncbigene ;
        m2r:variation ?variation .
    ?variation a m2r:Variation ;
        civico:variantOrigin ?variant_origin ;
        rdfs:label ?variation_label ;
        m2r:evidence / dct:references ?pubmed .
}
LIMIT 100
