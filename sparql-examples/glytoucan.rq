# Endpoint: https://rdfportal.org/primary/sparql
# Description: Obtain pairs of glycan ID and its entry URL

PREFIX glycan: <http://purl.jp/bio/12/glyco/glycan#>
PREFIX glytoucan: <http://www.glytoucan.org/glyco/owl/glytoucan#>

SELECT ?id
FROM <http://rdf.glycoinfo.org/glytoucan>
FROM <http://www.glytoucan.org/glyco/owl/glytoucan>
WHERE {
    VALUES ?Glycan__class { glycan:saccharide glycan:glycan_motif }
    ?Glycan a ?Glycan__class ;
        glytoucan:has_primary_id ?id .
}
LIMIT 100
